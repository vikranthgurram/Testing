pipeline {
    agent any
    parameters{
        string(name:'Env',defaultValue:'Test',description:'version to deploy')

    }
    stages {
        stage('Compile') {
            steps {
                script{
                    echo "Compiling the code"
                  }
            }
        }
        stage('UnitTest') {
            steps {
                script{
                    echo "testing the code in ${params.Env} env"
                }
            }
        }
        stage('Package') {
            steps {
                script{
                    echo "Packaging thr code"
                }
            }
        }
    }
}
