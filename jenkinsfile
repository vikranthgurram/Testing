pipeline {
    agent any
    parameters{
        string(name:'Env',defaultValue:'Test',description:'version to deploy')

    }
    stages {
        stage('compile') {
            steps {
                script{
                    echo "Compiling the code"
                  }
            }
        }
        stage('test') {
            steps {
                script{
                    echo "Running the test cases"
                }
            }
        }
        stage('package1') {
            steps {
                script{
                    echo "Packaging thr code"
                }
            }
        }
    }
}
